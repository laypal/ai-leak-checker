{
  "name": "ai-leak-checker",
  "version": "0.1.0",
  "description": "Browser extension to prevent accidental data leaks to AI chat platforms",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "tsc && node scripts/build-entries.js",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:unit:watch": "vitest --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:corpus": "tsx tests/corpus/run-corpus-test.ts",
    "test:property": "cd tests/property && python -m pytest -v",
    "test:coverage": "vitest run --coverage",
    "test:build": "vitest run tests/build/build-output.test.ts",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "husky install"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "ai-security",
    "data-leak-prevention",
    "dlp",
    "chatgpt",
    "claude"
  ],
  "author": "Lyall",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-web-extension": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "preact": "^10.19.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
